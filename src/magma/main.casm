section data {
    first = 100;
    second = 200;

    ZERO = 0;
    ONE = 1;
    TWO = 2;
}

section program {
    label __main__ {
        ldv main0;
        push;

        ldd ZERO;
        push;

        ldd second;
        push;

        ldd first;
        push;

        jp add;
    }

    label main0 {
        ldd first;
        out;
        halt;
    }

    label add {
        ldd %sp;
        out;

        jpac;
    }
}