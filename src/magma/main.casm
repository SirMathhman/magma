section data {
    temp = 0;

    first = 100;
    second = 200;

    ZERO = 0;
    ONE = 1;
    TWO = 2;
}

section program {
    label __main__ {
        ldd $sp;
        add ONE;
        stod $sp;

        ldv main0;
        stoi $sp;

        ldv add;
        jpac;
    }

    label main0 {
        halt;
    }

    label add {
        ldi $sp;
        jpac;
    }
}