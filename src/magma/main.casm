section data {
	cache = 0;
}
section program {
	label __main__ {
		stod cache;
		ldd $sp;
		addv 1;
		stod $sp;
		ldd cache;

		ldv 100;
		stoi $sp;

		ldd $sp;

		stod cache;
		ldd $sp;
		subv 1;
		stod $sp;
		ldd cache;

		stoi $sp;

		stod cache;
		ldd $sp;
		addv 2;
		stod $sp;
		ldd cache;

		ldi $sp;
		stoi $sp;
		stod cache;
		ldd $sp;
		subv 2;
		stod $sp;
		ldd cache;
		ldv 0;
		out;
		halt;
	}
}