section data {
	cache = 0;
}
section program {
	label __main__ {
		ldv 100;
		stoi $sp;
		stod cache;
		ldd $sp;
		addv 1;
		stod $sp;
		ldd cache;
		ldv 200;
		stoi $sp;
		stod cache;
		ldd $sp;
		addv 1;
		stod $sp;
		ldd cache;
		ldv 300;
		stoi $sp;
		stod cache;
		ldd $sp;
		subv 2;
		stod $sp;
		ldd cache;
		ldi $sp;
		out;
		halt;
	}
}
