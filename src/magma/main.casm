section data {
    first = 100;
    second = 200;
    ONE = 1;
}

section program {
    label __main__ {
        lda $sp;
        add ONE;
        stod $sp;

        ldv main_end;
        stoi $sp;
        jp add;
    }

    label main_end {
        lda first;
        out;
        halt;
    }

    label add {
        lda $sp;
        ldac;
        jpac;
    }
}