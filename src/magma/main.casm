section data {
    temp = 0;

    first = 100;
    second = 200;

    ZERO = 0;
    ONE = 1;
    TWO = 2;
}

section program {
    label __main__ {
        ldv main0;
        push;

        ldv add;
        jpac;
    }

    label main0 {
        ldd first;
        out;
        ldd second;
        out;
        halt;
    }

    label add {
        ldi $sp;
        jpac;
    }
}